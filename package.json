  "devDependencies": {
    "@types/node": "^24.3.1",
    "@vercel/ncc": "^0.38.4",
    "jest": "^29.7.0",
    "nock": "^14.0.11",
    "semistandard": "^17.0.0"
  },
  "semistandard": {
    "ignore": [
      "**/dist/**"
    ]
  },
  "overrides": {
