  "version": "3.1.2",
  "description": "Setup Terraform CLI for GitHub Actions",
  "license": "MPL-2.0",
  "publisher": "hashicorp",
  "main": "index.js",
  "engines": {
    "node": ">=24.0.0"
  },
  "repository": {
    "type": "git",
